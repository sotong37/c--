
Program keliling_lingkaran;
Uses crt;
Var 
       R,kel,phi:real;
Begin
Clrscr;
          Writeln(‘program menghitung keliling lingkaran’);
          Writeln(‘masukan  panjang jari jari lingkaran’);
          PHI:=3.14;
          KEL:=2*phi*r;
          Writeln(‘jadi keliling lingkaran adalah   :     ‘,kel);readln;
          Writeln(‘T E R I M A  K A S I H’);readln
End.
